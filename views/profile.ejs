  <h1 class="heading1 text-center">Fabulous Digital Managements</h1>
  <h2>Your Profile</h2>
  <div id="profile" class="text-center">
    <div class="row">
      <div class="col-md-6">
        <h5>Name:</h5>
      </div>
      <div class="col-md-6">
        <h5>
          <%=firstName%>
            <%=lastName%>
        </h5>
      </div>
    </div>
    <div class="row">
      <div class="col-md-6">
        <h5>User ID:</h5>
      </div>
      <div class="col-md-6">
        <h5>
          <%=username%>
        </h5>
      </div>
    </div>
    <div class="row">
      <div class="col-md-6">
        <h5>Email:</h5>
      </div>
      <div class="col-md-6">
        <h5>
          <%=email%>
            </h5>
      </div>
    </div>
    <div class="row">
      <div class="col-md-6">
        <h5>Phone no. :</h5>
      </div>
      <div class="col-md-6">
        <h5>
          <%=contact%>
            </h5>
      </div>
    </div>
    <div class="row">
      <div class="col-md-6">
        <h5>Age :</h5>
      </div>
      <div class="col-md-6">
        <h5>
          <%=age%>
            </h5>
      </div>
    </div>
    <div class="row">
      <div class="col-md-6">
        <h5>Gender:</h5>
      </div>
      <div class="col-md-6">
        <h5>
          <%=gender%>
            </h5>
      </div>
    </div>
    <div class="row">
      <div class="col-md-6">
        <h5>City :</h5>
      </div>
      <div class="col-md-6">
        <h5>
          <%=city%>
            </h5>
      </div>
    </div>
  </div>
  <div class="text-center" style="margin-top: 20px;">
    <button type="button" id="showEvents" class="btn btn-dark">Show My Events</button>
  </div>

  <div id="events" class="text-center">

  </div>


